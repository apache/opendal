{
  "name": "opendal",
  "author": "OpenDAL Contributors <dev@opendal.apache.org>",
  "version": "0.30.3",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "Open Data Access Layer: Access data freely, painlessly, and efficiently",
  "repository": "git@github.com/apache/incubator-opendal.git",
  "napi": {
    "name": "opendal",
    "package": {
      "name": "@opendal/lib"
    }
  },
  "keywords": [
    "api",
    "fs",
    "file system",
    "gcs",
    "ghac",
    "http",
    "ipmfs",
    "memory",
    "obs",
    "oss",
    "s3",
    "storage",
    "webdav",
    "webhdfs"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "generated.js"
  ],
  "devDependencies": {
    "@cucumber/cucumber": "^9.0.1",
    "@napi-rs/cli": "^2.15.0",
    "@swc-node/register": "^1.6.2",
    "@swc/core": "^1.3.38",
    "@types/node": "^18.14.5",
    "prettier": "^2.8.4",
    "typedoc": "^0.23.26",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js generated.js --pipe 'node scripts/appendLicenseHeader.js'",
    "build:debug": "napi build --platform --js generated.js --pipe 'node scripts/appendLicenseHeader.js'",
    "docs": "typedoc",
    "format": "prettier --write .",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cucumber-js",
    "version": "napi version"
  },
  "prettier": {
    "overrides": [
      {
        "files": "./**/*.{js,ts,mjs}",
        "options": {
          "printWidth": 120,
          "semi": false,
          "trailingComma": "all",
          "singleQuote": true,
          "arrowParens": "always",
          "parser": "typescript"
        }
      }
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "packageManager": "yarn@3.4.1+sha224.cca891d4a8671d4898aba3426674bb734dbbf88cef82dd4dacd71c9f",
  "resolutions": {
    "@napi-rs/cli@^2.15.0": "patch:@napi-rs/cli@npm%3A2.15.0#./.yarn/patches/@napi-rs-cli-npm-2.15.0-6ff0615d2c.patch"
  }
}
